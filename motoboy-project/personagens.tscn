script/source = "extends CanvasLayer


@onready var label := get_node(\"Label\")

func _input(event: InputEvent):
	if event.is_action_pressed(\"ui_end\"):
		get_tree().paused = !get_tree().paused
		label.visible = get_tree().paused
		
		

func _on_button_pressed() :
	get_tree().quit()
"

[sub_resource type="GDScript" id="GDScript_b3p1w"]
script/source = "extends CharacterBody2D

var aceleracao:float = 100
var desaceleracao:float = 600
var gravidade:float
var jump_height:float = 100
var pulo_speed:float = 200
var time_apice_pulo:float = 0.3
var velocidade_maxima_x:float = 100
var velocidade_maxima_y:float = 700

func _ready():
	$AnimatedSprite2D.play(\"idle\")
	pulo_speed = (jump_height*2) / time_apice_pulo
	gravidade = (jump_height*2) / pow(time_apice_pulo, 2)

func _physics_process(delta):
	velocity.y += gravidade * delta

	if Input.is_action_pressed(\"ui_left\"):
		$AnimatedSprite2D.flip_h = true
		if Input.is_action_pressed(\"run\"):
			velocity.x = -velocidade_maxima_x * 2
		else:
			velocity.x = max(velocity.x - aceleracao * delta, -velocidade_maxima_x)

	elif Input.is_action_pressed(\"ui_right\"):
		$AnimatedSprite2D.flip_h = false
		if Input.is_action_pressed(\"run\"):
			velocity.x = velocidade_maxima_x * 2
		else:
			velocity.x = min(velocity.x + aceleracao * delta, velocidade_maxima_x)

	else:
		if velocity.x > 0:
			velocity.x = max(velocity.x - desaceleracao * delta, 0)
		elif velocity.x < 0:
			velocity.x = min(velocity.x + desaceleracao * delta, 0)

	if Input.is_action_just_pressed(\"ui_jump\") and is_on_floor():
		velocity.y = -pulo_speed

	move_and_slide()
	
	if not is_on_floor(): 
		if velocity.y < 0: 
			$AnimatedSprite2D.play(\"animation_jump\")
		else:
			$AnimatedSprite2D.play(\"animation_fall\")
	else:
		if velocity.x == 0:
			$AnimatedSprite2D.play(\"idle\")
		elif abs(velocity.x) > velocidade_maxima_x: 
			$AnimatedSprite2D.play(\"animation_run\")
		else:
			$AnimatedSprite2D.play(\"animation_walk\")
"

